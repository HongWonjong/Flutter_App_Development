{
  "hosting": {
    "public": "build/web"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "extensions": {
    "firestore-genai-chatbot": "googlecloud/firestore-genai-chatbot@0.0.10",
    "firestore-genai-title": "googlecloud/firestore-genai-chatbot@0.0.10"
  }
}
